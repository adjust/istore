BEGIN;
-- functions_plain should add istores;
-- ./spec/functions_plain_spec.rb:22;
CREATE EXTENSION istore;
SELECT add('1=>1, 2=>1'::istore, '1=>1, 2=>1'::istore);
        add         
--------------------
 "1"=>"2", "2"=>"2"
(1 row)

SELECT add('1=>1, 2=>1'::istore, '-1=>1, 2=>1'::istore);
              add              
-------------------------------
 "-1"=>"1", "1"=>"1", "2"=>"2"
(1 row)

SELECT add('1=>1, 2=>1'::istore, '-1=>-1, 2=>1'::istore);
              add               
--------------------------------
 "-1"=>"-1", "1"=>"1", "2"=>"2"
(1 row)

SELECT add('-1=>1, 2=>1'::istore, '-1=>-1, 2=>1'::istore);
         add         
---------------------
 "-1"=>"0", "2"=>"2"
(1 row)

SELECT add('-1=>-1, 2=>1'::istore, '-1=>-1, 2=>1'::istore);
         add          
----------------------
 "-1"=>"-2", "2"=>"2"
(1 row)

SELECT add('-1=>-1, 2=>1'::istore, 1);
         add         
---------------------
 "-1"=>"0", "2"=>"2"
(1 row)

SELECT add('-1=>-1, 2=>1'::istore, -1);
         add          
----------------------
 "-1"=>"-2", "2"=>"0"
(1 row)

SELECT add('-1=>-1, 2=>1'::istore, 0);
         add          
----------------------
 "-1"=>"-1", "2"=>"1"
(1 row)

ROLLBACK;
